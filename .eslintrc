{
  "extends": [
    "oclif",
    "oclif-typescript"
  ],
  "rules": {
    // "no-console": "off",
    "complexity": "off",
    "indent": "off",
    "object-curly-spacing": "off",
    "padded-blocks": "off",
    "no-multiple-empty-lines": "off",
    "array-bracket-spacing": "off",
    "no-multi-assign": "off",
    "unicorn/prevent-abbreviations": "off",
    "unicorn/prefer-module": "off"
  }
}
