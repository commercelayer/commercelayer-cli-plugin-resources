{
  "name": "@commercelayer/cli-plugin-resources",
  "description": "Commerce Layer CLI Resources plugin",
  "version": "5.7.1",
  "author": "Pierluigi Viti <pierluigi@commercelayer.io>",
  "bugs": "https://github.com/commercelayer/commercelayer-cli-plugin-resources/issues",
  "engines": {
    "node": ">=16"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/commercelayer/commercelayer-cli-plugin-resources",
  "keywords": [
    "oclif",
    "ecommerce",
    "cli",
    "commercelayer"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "cl-resources",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "plugins": [],
    "topics": {
      "resources": {
        "description": "Execute CRUD operations on API resources"
      },
      "order": {
        "description": "Execute special operations on orders",
        "hidden": true
      }
    },
    "repositoryPrefix": "<%- repo %>/blob/main/<%- commandPath %>",
    "additionalHelpFlags": [
      "-h"
    ],
    "topicSeparator": ":"
  },
  "repository": "commercelayer/commercelayer-cli-plugin-resources",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "npm run resources && rm -rf lib && tsc -b && oclif manifest && npm run readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "readme": "cl-cli-dev readme --plugin --bin=commercelayer && git add README.md",
    "resources": "npm update @commercelayer/sdk && ts-node src/util/resources/build.ts",
    "preinstall": "npx only-allow pnpm"
  },
  "types": "lib/index.d.ts",
  "devDependencies": {
    "@commercelayer/cli-dev": "0.1.10",
    "@oclif/plugin-help": "^5.1.16",
    "@oclif/test": "^2.2.6",
    "@types/chai": "^4.3.3",
    "@types/inquirer": "^8.2.4",
    "@types/lodash": "^4.14.186",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.11.4",
    "@types/node-notifier": "^8.0.2",
    "chai": "^4.3.6",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "inflector-js": "^1.0.1",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@commercelayer/cli-core": "^1.6.1",
    "@commercelayer/js-auth": "^2.3.0",
    "@commercelayer/sdk": "^4.16.0",
    "@oclif/core": "^1.19.1",
    "axios": "^0.27.2",
    "inquirer": "^8.2.5",
    "json-2-csv": "^3.17.2",
    "lodash": "^4.17.21",
    "node-notifier": "^10.0.1",
    "tslib": "^2.4.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
